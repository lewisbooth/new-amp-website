.language-html, .language-javascript, .language-css, .language-react, .language-bash
  position relative

.language-html:before, .language-css:before, .language-javascript:before, .language-react:before, .language-bash:before
  position absolute
  font-family montserratSemiBold
  color darkGrey
  font-size 1.5rem
  top -1.5rem
  left 0
  width 100%
  padding 0.2em 0.5em
  box-sizing border-box

.language-html:before
  content "HTML"
  background #f92672

.language-css:before
  content "CSS"
  background #a6e22e

.language-javascript:before
  content "JavaScript"
  background #F7DF1E

.language-react:before
  content "React"
  background #61dafb

.language-bash:before
  content "Bash"
  background #2A3539
  color #ffffff

.mjml:before
  content "MJML"
  background #f46d4c

.mjml .nt
  color #f46d4c

.highlight span
  line-height 1.5

